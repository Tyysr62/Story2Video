{
  "cli": {
    "requireCommit": true
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "20.10.0",
      "env": {
        "EXPO_USE_PNPM": "1"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "node": "20.10.0",
      "env": {
        "EXPO_USE_PNPM": "1"
      }
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "node": "20.10.0",
      "env": {
        "EXPO_USE_PNPM": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
